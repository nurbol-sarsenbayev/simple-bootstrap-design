{
  "name": "simple-bootstrap-design",
  "description": "Простой адаптивный Bootstrap шаблон для обучения",
  "authors": [
    "Nurbol Sarsenbayev"
  ],
  "install": {
    "path": "app/libs/", // указывается папка куда bower-installer перемещает файлы 
    "sources": { // переопределяем значение свойства main bower.json файла пакета 
      "bootstrap-grid": [
        "bower_components/bootstrap-grid/dist/bootstrap-grid.min.css"
      ],
      "font-awesome": [
        "bower_components/font-awesome/css/font-awesome.min.css"
      ]
    }
  },
  "license": "ISC",
  "keywords": [
    "gulp",
    "simple",
    "простая",
    "верстка",
    "bootstrap"
  ],
  "dependencies": {
    "jquery": "^3.2.1",
    "bootstrap-grid": "^3.3.6-beta.1",
    "font-awesome": "fontawesome#^4.7.0"
  }
}
